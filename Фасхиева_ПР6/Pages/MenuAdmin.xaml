<Page x:Class="Фасхиева_ПР6.MenuAdmin"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Фасхиева_ПР6"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MenuAdmin">

    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <StackPanel Orientation="Horizontal">
                <Button Name="btUsers" Content="Пользователи" Click="btUsers_Click" Template="{StaticResource Btn3}"/>
                <Button Name="btGroup" Content="Группы" Click="btGroup_Click" Template="{StaticResource Btn3}"/>
                <Button Name="btPersonal" Content="Личный кабинет" Click="btPersonal_Click" Template="{StaticResource Btn3}"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
